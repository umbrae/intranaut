{
  "manifest_version": 2,
  "name": "Intranaut",
  "short_name": "Intranaut",
  "description": "An instant intranet.",
  "version": "0.3",
  "author": "Chris Dary <umbrae@gmail.com>",
  "icons": { "16": "build/img/icon16.png",
           "48": "build/img/icon48.png",
          "128": "build/img/icon128.png" },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "build/html/tab.html"
  },
  "options_page": "build/html/options.html"
}
