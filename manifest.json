{
  "manifest_version": 2,
  "name": "Intranaut",
  "description": "An instant intranet.",
  "version": "0.2",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "build/html/tab.html"
  },
  "options_page": "build/html/options.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
